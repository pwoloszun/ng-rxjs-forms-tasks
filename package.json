{
  "name": "ng-ngrx-fundamentals-tasks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4000 --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --runInBand --no-watchman",
    "test:watch": "jest --watch --maxWorkers=1 --no-watchman",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "npm run cypress:run",
    "json-server": "json-server ./server/db.json -p 4100 -r ./server/config/routes.json -S ./server/snapshots"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/flex-layout": "~12.0.0-beta.34",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@ngrx/data": "^12.0.0",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/entity": "^12.0.0",
    "@ngrx/router-store": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "@ngrx/store-devtools": "^12.0.0",
    "faker": "~5.5.3",
    "hammerjs": "~2.0.8",
    "immer": "~9.0.2",
    "json-server": "~0.16.3",
    "lodash": "~4.17.21",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@ngrx/schematics": "^12.0.0",
    "@testing-library/angular": "~10.7.1",
    "@testing-library/user-event": "^13.1.9",
    "@types/faker": "~5.5.5",
    "@types/jest": "~26.0.23",
    "@types/lodash": "~4.14.170",
    "cypress": "~7.3.0",
    "jest": "~26.6.3",
    "jest-preset-angular": "~8.4.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.2.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "cypress"
    ]
  }
}
